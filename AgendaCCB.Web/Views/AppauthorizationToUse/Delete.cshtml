@model AgendaCCB.Web.Models.AppAuthorizationToUseViewModel

@{
    ViewData["Title"] = "Remover autorização APP";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Remover autorização APP</h2>
<ol class="breadcrumb">
    <li class="breadcrumb-item">@Html.ActionLink("Index", "Index", "AppauthorizationToUse")</li>
    <li class="breadcrumb-item active">Detalhes</li>
</ol>

<div class="panel panel-default panel-table">
    <div class="panel-heading">
        <div class="row">
            <div class="col col-xs-6">
                <h3 class="panel-title">Dados autorização APP</h3>
            </div>
        </div>
    </div>
    <div class="panel-body">
        <table class="table table-bordered table-striped">
            <tbody>
                <tr>
                    <th class="col-md-3">
                        @Html.DisplayNameFor(model => model.PhoneNumber)
                    </th>
                    <td>
                        @Html.DisplayFor(model => model.PhoneNumber)
                    </td>
                </tr>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Token)
                    </th>
                    <td>
                        @Html.DisplayFor(model => model.Token)
                    </td>
                </tr>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Created)
                    </th>
                    <td>
                        @Html.DisplayFor(model => model.Created)
                    </td>
                </tr>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Used)
                    </th>
                    <td>
                        @Html.DisplayFor(model => model.Used)
                    </td>
                </tr>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.UserCreator)
                    </th>
                    <td>
                        @Html.DisplayFor(model => model.UserCreator)
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
<h3>Tem certeza que deseja remover?</h3>
@using (Html.BeginForm())
{
    <div class="form-actions">
        <input type="submit" value="Excluir" class="btn btn-danger btn-submit" />
        <a href="@Url.Action("Index")" class="btn btn-default">Voltar para a Lista</a>
    </div>
}